'List of object ids and associated names
CONST MAX_NUM_OBJ_TYPES% = 100

CONST OBJ_TYPE_OBJECT%=0
CONST OBJ_TYPE_VIEW_OBJECT%=1
CONST OBJ_TYPE_TEXT_ENTRY%=2

CONST OBJ_TYPE_TEST%=3
CONST OBJ_TYPE_TEST2%=4

CONST OBJ_TYPE_SS_BULLET%=5
CONST OBJ_TYPE_SS_SAUCER%=6
CONST OBJ_TYPE_SS_EXPLOSION%=7
CONST OBJ_TYPE_SS_HERO%=8
CONST OBJ_TYPE_SS_POINTS%=9
CONST OBJ_TYPE_SS_GAMESTART%=10
CONST OBJ_TYPE_SS_GAMEOVER%=11
CONST OBJ_TYPE_SS_STAR%=12

DIM OBJ_NAMES$(MAX_NUM_OBJ_TYPES%) LENGTH 16

OBJ_NAMES$(OBJ_TYPE_OBJECT%) = "Object"
OBJ_NAMES$(OBJ_TYPE_VIEW_OBJECT%) = "View Object"
OBJ_NAMES$(OBJ_TYPE_TEXT_ENTRY%) = "Text Entry"
OBJ_NAMES$(OBJ_TYPE_TEST%) = "Test Object"
OBJ_NAMES$(OBJ_TYPE_TEST2%) = "Test Object 2"
OBJ_NAMES$(OBJ_TYPE_SS_BULLET%) = "SS Bullet"
OBJ_NAMES$(OBJ_TYPE_SS_HERO%) = "SS Hero"
OBJ_NAMES$(OBJ_TYPE_SS_SAUCER%) = "SS Saucer"
OBJ_NAMES$(OBJ_TYPE_SS_EXPLOSION%) = "SS Explosion"
OBJ_NAMES$(OBJ_TYPE_SS_POINTS%) = "SS Points"

